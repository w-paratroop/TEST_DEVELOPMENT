{
  "name": "web",
  "version": "0.7.1",
  "description": "parachute web develop environment - webpack, babel, dart sass",
  "main": "index.js",
  "scripts": {
    "start": "webpack-cli serve --mode development --config scripts/webpack.config.dev.js",
    "build": "webpack --mode production --config scripts/webpack.config.build.js",
    "express": "node scripts/express.server.js",
    "scss:lint": "stylelint src/**/*.scss src-scss/**/*.scss --config .stylelintrc.yml",
    "scss:lint:fix": "stylelint src/**/*.scss src-scss/**/*.scss --config .stylelintrc.yml --fix",
    "js:lint": "eslint './src/**/*.js' 'src-babels/**/*.{js,jsx}'",
    "js:lint:fix": "eslint 'src/**/*.js' 'src-babels/**/*.{js,jsx}' --fix",
    "js:format": "prettier --write 'src/**/*.{js,jsx}' 'src-babels/**/*.{js,jsx}'",
    "scss:format": "prettier --write 'src/**/*.scss' 'src-scss/**/*.scss'",
    "md:format": "prettier --write './README.md' '_readme/**/*.md' '_guide/**/*.md'",
    "scss:format:debug": "prettier 'src/**/*.scss' 'src-scss/**/*.scss' --debug-check",
    "js:format:debug": "prettier 'src/**/*.{js,jsx}' 'src-babels/**/*.{js,jsx}' --debug-check",
    "test": "jest",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,scss}": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "keywords": [
    "web",
    "scss",
    "ES2020"
  ],
  "engines": {
    "node": ">= 16.14.2",
    "npm": ">= 8.5.0"
  },
  "browserslist": [
    "> 0.2% and not dead"
  ],
  "author": "@taikiken",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@squoosh/lib": "^0.5.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.29.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "globule": "^1.3.4",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "imagemin": "^8.0.1",
    "imagemin-svgo": "^10.0.1",
    "img-loader": "^4.0.0",
    "intersection-observer": "^0.12.2",
    "mini-css-extract-plugin": "^2.7.2",
    "node-ssi": "^0.3.2",
    "object.assign": "^4.1.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-scss": "^4.0.6",
    "promise-polyfill": "^8.3.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.3",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint-webpack-plugin": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-order": "^6.0.2",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.4.0",
    "webpack-remove-empty-scripts": "^1.0.1"
  }
}
